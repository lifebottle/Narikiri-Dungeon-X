    <Entry>
      <PointerOffset>37020</PointerOffset>
      <VoiceId>&lt;VCT_003_090_001&gt;</VoiceId>
      <JapaneseText>あの幻……あの子はどうして
町の中をさまよって……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>1</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>37132</PointerOffset>
      <VoiceId>&lt;VCT_003_090_002&gt;</VoiceId>
      <JapaneseText>そうだ……捨てられたんだ……
お母さんに、捨てられたんだ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>2</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>37740</PointerOffset>
      <VoiceId>&lt;VCT_003_090_003&gt;</VoiceId>
      <JapaneseText>戦争があって……それで、
すごく長くて寒い冬があって……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>3</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>38012</PointerOffset>
      <VoiceId>&lt;VCT_003_090_004&gt;</VoiceId>
      <JapaneseText>もうこれ以上、お母さんは
その子を育てることができなくて……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>4</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>38420</PointerOffset>
      <VoiceId>&lt;VCT_003_090_005&gt;</VoiceId>
      <JapaneseText>一番、いらない子だったわたしを捨てた……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>5</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>38588</PointerOffset>
      <VoiceId>&lt;VCT_003_090_006&gt;</VoiceId>
      <JapaneseText>……あれ？　なんでこんなこと、
わたし知ってるの？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>6</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>38880</PointerOffset>
      <VoiceId>&lt;VCT_003_090_007&gt;</VoiceId>
      <JapaneseText>幻……？　もしかして幻じゃない……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>7</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>39284</PointerOffset>
      <VoiceId>&lt;VCT_003_090_008&gt;</VoiceId>
      <JapaneseText>……わかんない……わかんないよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>8</Id>
      <Status>To Do</Status>
    </Entry>
