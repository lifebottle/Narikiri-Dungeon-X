    <Entry>
      <PointerOffset>37120</PointerOffset>
      <VoiceId>&lt;VCT_003_089_001&gt;</VoiceId>
      <JapaneseText>&lt;RND_P&gt;さんの指輪、便利ですよね。
おしゃれと実用が兼ねられてて</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>1</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>37572</PointerOffset>
      <VoiceId>&lt;VCT_003_089_002&gt;</VoiceId>
      <JapaneseText>そうね、便利って言えば
便利だけど……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>2</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>37804</PointerOffset>
      <VoiceId>&lt;VCT_003_089_003&gt;</VoiceId>
      <JapaneseText>……だけど？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>3</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>38052</PointerOffset>
      <VoiceId>&lt;VCT_003_089_004&gt;</VoiceId>
      <JapaneseText>指輪は指輪だからこそ、
意味があると思うんだよね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>4</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>38372</PointerOffset>
      <VoiceId>&lt;VCT_003_089_005&gt;</VoiceId>
      <JapaneseText>？？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>5</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>38524</PointerOffset>
      <VoiceId>&lt;VCT_003_089_006&gt;</VoiceId>
      <JapaneseText>こういうものは、キレイなだけで
充分な意味があんの</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>6</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>39032</PointerOffset>
      <VoiceId>&lt;VCT_003_089_007&gt;</VoiceId>
      <JapaneseText>余計な価値をつけたことで、
指輪は指輪じゃなくなった</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>7</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>39380</PointerOffset>
      <VoiceId>&lt;VCT_003_089_008&gt;</VoiceId>
      <JapaneseText>&lt;MEL&gt;ちゃんにはまだわかんないかな？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>8</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>39660</PointerOffset>
      <VoiceId>&lt;VCT_003_089_009&gt;</VoiceId>
      <JapaneseText>うーん……わかりません</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>9</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>40820</PointerOffset>
      <VoiceId>&lt;VCT_003_089_010&gt;</VoiceId>
      <JapaneseText>もし、こんな力が指輪になかったら、
ダオスを追いかけることもなかったし</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>10</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>41324</PointerOffset>
      <VoiceId>&lt;VCT_003_089_011&gt;</VoiceId>
      <JapaneseText>私はダオスのやってることを知らないまま、彼の
思い出をこの指輪とともに抱いて生きていた……か</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>11</Id>
      <Status>To Do</Status>
    </Entry>
