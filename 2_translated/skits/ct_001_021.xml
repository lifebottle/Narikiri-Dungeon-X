    <Entry>
      <PointerOffset>37616</PointerOffset>
      <VoiceId>&lt;VCT_001_021_001&gt;</VoiceId>
      <JapaneseText>ね、&lt;DIO&gt;……ダオス戦役のことが
書いてある本ってある？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>1</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>37948</PointerOffset>
      <VoiceId>&lt;VCT_001_021_002&gt;</VoiceId>
      <JapaneseText>あるけど……全部、家だよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>2</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>38360</PointerOffset>
      <VoiceId>&lt;VCT_001_021_003&gt;</VoiceId>
      <JapaneseText>&lt;MEL&gt;&lt;MEL&gt;〜、おらの中に『ダオス戦役のひみつ』、
『こども歴史大百科』があっただよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>3</SpeakerId>
      <Id>3</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>40032</PointerOffset>
      <VoiceId>&lt;VCT_001_021_004&gt;</VoiceId>
      <JapaneseText>あ……それ、オレの本！
何、勝手に持ってきてんだ！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>4</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>40660</PointerOffset>
      <VoiceId>&lt;VCT_001_021_005&gt;</VoiceId>
      <JapaneseText>ありがと、アルベルトさん</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>5</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>41512</PointerOffset>
      <VoiceId>&lt;VCT_001_021_006&gt;</VoiceId>
      <JapaneseText>……ふんふん……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>6</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>41852</PointerOffset>
      <VoiceId>&lt;VCT_001_021_007&gt;</VoiceId>
      <JapaneseText>でも、どーしたんだよ、
今まで興味なかったくせに</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>7</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>42188</PointerOffset>
      <VoiceId>&lt;VCT_001_021_008&gt;</VoiceId>
      <JapaneseText>みんなのしてる話が全然、わかんないから、
このままじゃいけないと思ったんだ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>8</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>42864</PointerOffset>
      <VoiceId>&lt;VCT_001_021_009&gt;</VoiceId>
      <JapaneseText>こういうことは、ちゃんと知っておかないと
ダメなんだよ、きっと</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>9</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>43372</PointerOffset>
      <VoiceId>&lt;VCT_001_021_010&gt;</VoiceId>
      <JapaneseText>相変わらず&lt;MEL&gt;って真面目だよな……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>10</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>43540</PointerOffset>
      <VoiceId>&lt;VCT_001_021_011&gt;</VoiceId>
      <JapaneseText>あ〜、どこぞのチビ助と違って、な！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>3</SpeakerId>
      <Id>11</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>44456</PointerOffset>
      <VoiceId>&lt;VCT_001_021_012&gt;</VoiceId>
      <JapaneseText>うるせー！　本棚に作り変えちまうぞ！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>12</Id>
      <Status>To Do</Status>
    </Entry>
