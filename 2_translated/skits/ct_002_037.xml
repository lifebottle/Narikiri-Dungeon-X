    <Entry>
      <PointerOffset>37432</PointerOffset>
      <VoiceId>&lt;VCT_002_037_001&gt;</VoiceId>
      <JapaneseText>&lt;CST&gt;って、
あんなに気さくな人なんだな</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>1</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>37648</PointerOffset>
      <VoiceId>&lt;VCT_002_037_002&gt;</VoiceId>
      <JapaneseText>どうして？</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>2</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>37888</PointerOffset>
      <VoiceId>&lt;VCT_002_037_003&gt;</VoiceId>
      <JapaneseText>目つきが悪くてすごく性格ねじ曲がってて意地悪で、
文句ばっか言ってる人で……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>3</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>39004</PointerOffset>
      <VoiceId>&lt;VCT_002_037_004&gt;</VoiceId>
      <JapaneseText>おいおい、誰のことだよ、それ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>3</SpeakerId>
      <Id>4</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>39348</PointerOffset>
      <VoiceId>&lt;VCT_002_037_005&gt;</VoiceId>
      <JapaneseText>&lt;CST&gt;だよ……って、
おわっ！！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>5</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>40304</PointerOffset>
      <VoiceId>&lt;VCT_002_037_006&gt;</VoiceId>
      <JapaneseText>ヒドいな、誰だよ、んなこと言ってんの</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>3</SpeakerId>
      <Id>6</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>40880</PointerOffset>
      <VoiceId>&lt;VCT_002_037_007&gt;</VoiceId>
      <JapaneseText>え？　読んだ本に書いてたぜ。
……おい、出て来いタンス野郎！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>7</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>41924</PointerOffset>
      <VoiceId>&lt;VCT_002_037_008&gt;</VoiceId>
      <JapaneseText>ぬわー！　誰に向かってタンスとか言ってんだ！
いい加減、どつきまわして、ひぃひぃ言わ……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>4</SpeakerId>
      <Id>8</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>43272</PointerOffset>
      <VoiceId>&lt;VCT_002_037_009&gt;</VoiceId>
      <JapaneseText>このへんにあの本しまったよな……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>9</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>43828</PointerOffset>
      <VoiceId>&lt;VCT_002_037_010&gt;</VoiceId>
      <JapaneseText>ぬひゃ……。こ、こら、おめぇ……、何すっだ！　ぐ。
お、おらの……、ひゃは……体ん中、まさぐるでね！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>4</SpeakerId>
      <Id>10</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>45076</PointerOffset>
      <VoiceId>&lt;VCT_002_037_011&gt;</VoiceId>
      <JapaneseText>あったあった、この本……
えっと……作者はＡ・クライン……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>11</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>45892</PointerOffset>
      <VoiceId>&lt;VCT_002_037_012&gt;</VoiceId>
      <JapaneseText>……&lt;ARC&gt;じゃねぇか！　あのヤローっ！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>3</SpeakerId>
      <Id>12</Id>
      <Status>To Do</Status>
    </Entry>
