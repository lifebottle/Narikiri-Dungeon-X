    <Entry>
      <PointerOffset>37332</PointerOffset>
      <VoiceId>&lt;VCT_001_003_001&gt;</VoiceId>
      <JapaneseText>&lt;MEL&gt;、ちゃんと必要なもの
買ったね</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>1</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>38044</PointerOffset>
      <VoiceId>&lt;VCT_001_003_002&gt;</VoiceId>
      <JapaneseText>&lt;DIO&gt;、ほら、
ボタン掛け違えてる</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>2</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>38692</PointerOffset>
      <VoiceId>&lt;VCT_001_003_003&gt;</VoiceId>
      <JapaneseText>エト助は、ふたりの保護者みたいだなや</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>3</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>40124</PointerOffset>
      <VoiceId>&lt;VCT_001_003_004&gt;</VoiceId>
      <JapaneseText>そうだよ！
ぼくはふたりのお母さんだもん！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>4</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>40724</PointerOffset>
      <VoiceId>&lt;VCT_001_003_005&gt;</VoiceId>
      <JapaneseText>へん、エトが
母ちゃんなもんかよ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>3</SpeakerId>
      <Id>5</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>41208</PointerOffset>
      <VoiceId>&lt;VCT_001_003_006&gt;</VoiceId>
      <JapaneseText>あはは〜〜ん。
おめぇ、ほんとはエト助のこと、大好きなんだべ</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>6</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>43176</PointerOffset>
      <VoiceId>&lt;VCT_001_003_007&gt;</VoiceId>
      <JapaneseText>うるせー！　タンス野郎は黙ってろ！！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>3</SpeakerId>
      <Id>7</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>44564</PointerOffset>
      <VoiceId>&lt;VCT_001_003_008&gt;</VoiceId>
      <JapaneseText>はぁん？　誰に向かってタンス言ってんだど！
おめぇなんぞ、毛も生えてねぇ、ハゲぼうずのくせが！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>8</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>46292</PointerOffset>
      <VoiceId>&lt;VCT_001_003_009&gt;</VoiceId>
      <JapaneseText>ほら、ふたりともけんかしちゃだめ！</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>1</SpeakerId>
      <Id>9</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>47312</PointerOffset>
      <VoiceId>&lt;VCT_001_003_010&gt;</VoiceId>
      <JapaneseText>す……すまねーだ、母ちゃん</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>2</SpeakerId>
      <Id>10</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>47868</PointerOffset>
      <VoiceId>&lt;VCT_001_003_011&gt;</VoiceId>
      <JapaneseText>あれ……アルベルトさんまで……</JapaneseText>
      <EnglishText/>
      <Notes/>
      <SpeakerId>4</SpeakerId>
      <Id>11</Id>
      <Status>To Do</Status>
    </Entry>
